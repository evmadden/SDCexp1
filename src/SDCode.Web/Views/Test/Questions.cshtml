@model TestQuestionsViewModel
@{
    ViewData["Title"] = "Questions";
}

@await Html.PartialAsync("~/Views/Shared/LogoBanner.cshtml")

    <form action="@Url.Action("End", "Test")" method="POST">
        <input type="hidden" name="participantID" value="@Model.ParticipantID" />
        <input type="hidden" name="testName" value="@Model.TestName" />

    <h1 class="display-4; text-center;">Test Phase Questions</h1>
    <br/>
    <p>This completes the TEST PHASE.</p>
    <div>
        <div>
            <h1 class="display-4; text-center;">Questions</h1>
            <p>Please explain any problems that occurred during the TEST PHASE.</p>
        </div>
            <div>
                Why were images obscured? <input type="text" name="obscuredReason" class="longSingleLine" required />
            </div>
    </div>
    <p>Press the "Next" button to finish to the TEST PHASE.</p>
<div style="text-align: right;">
    <input style="margin-right: 50px;" type="submit" value="Next"/>
</div>
    </form>
